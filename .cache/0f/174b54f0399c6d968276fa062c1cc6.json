line":138,"column":15}}],"sources":{"css/cursor.scss":".circle-cursor {\n  position: absolute;\n  width: 1.2vw;\n  height: 1.2vw;\n  border-radius: 50%;\n  border: 1px solid $saffron;\n  opacity: 0.8;\n  transition: transform 0.2s cubic-bezier(0.075, 0.82, 0.165, 1);\n  pointer-events: none;\n  z-index: 2;\n}\n\n.cursor-down {\n  opacity: 1;\n  background: $saffron;\n  transform: scale(0.8);\n}\n\n.cursor-up {\n  transform: scale(1);\n}\n","css/variables.scss":"$saffron: #ff9933;\n$grey: #414042;\n","css/style.scss":"@import \"./variables.scss\";\n@import \"./cursor.scss\";\n\n@font-face {\n  font-family: \"Apfel Grotesk\";\n  src: url(\"../assets/apfelGrotezk/ApfelGrotezk-Regular.woff2\") format(\"woff2\");\n}\n\n* {\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n}\nbody {\n  cursor: none;\n  color: $saffron;\n  font-size: 12px;\n  font-family: \"Apfel Grotesk\";\n  margin: 0;\n  padding: 0;\n  background: linear-gradient(-95deg, #f4f5f4, #e4e1e1);\n  font-size: 16px;\n  background-repeat: no-repeat;\n  background-attachment: fixed;\n  height: 100vh;\n}\n\na {\n  text-decoration: none;\n  cursor: none;\n  transition: transform 0.2s cubic-bezier(0.075, 0.82, 0.165, 1);\n}\n\n.menu {\n  width: 100%;\n  height: 30%;\n  position: relative;\n  top: 0;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.menu a {\n  width: 10%;\n  margin: 2em;\n  color: $saffron;\n  text-align: center;\n}\n\n.menu a:hover {\n  opacity: 0.5;\n}\n\n.menu a.logo {\n  width: 15% !important;\n}\n\n.st0 {\n  width: 20%;\n  fill: $saffron;\n}\n\n.content-container {\n  padding-top: 5em;\n  width: 100%;\n  height: auto;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n\n  flex-direction: row;\n}\n\n.content-container a {\n  width: auto;\n  padding: 1em;\n}\n\nimg {\n  max-height: 500px;\n  max-width: 100%;\n}\n\n.info-container {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  margin: 1em 0;\n}\n\n.info-container p {\n  margin: 0;\n}\n\n.about-container {\n  display: flex;\n  flex-direction: row;\n  padding: 3em;\n}\n.about-container p {\n  margin: 0.5em;\n  color: $grey;\n}\n\n.about-container {\n  display: flex;\n  justify-content: center;\n}\n\n.about-container > .info-container {\n  width: 33%;\n}\n\n.about-container > .portrait-container {\n  width: 40%;\n}\n\n.about-container p.contact {\n  margin: 0.5em;\n  color: $saffron;\n}\n\n.portrait-container {\n  display: flex;\n  justify-content: center;\n}\n.info-box-label,\n.info-box-info {\n  display: flex;\n  justify-content: center;\n  flex-direction: column;\n  align-items: center;\n}\n.info-box-label {\n  padding-bottom: 1em;\n}\n\n.info-box-year {\n  font-weight: bold;\n}\n\n.resin-gallery {\n  width: 100%;\n}\n"},"lineCount":null}},"error":null,"hash":"cfea8741c0770fdefe80bf357549530a","cacheData":{"env":{}}}